{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "vitest/globals",
    ]
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules/**/*",
    "dist/**/*"
  ],
}